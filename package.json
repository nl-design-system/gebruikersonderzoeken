    "eslint-plugin-perfectionist": "4.12.3",
    "eslint-plugin-react": "7.37.5",
    "globals": "16.0.0",
    "husky": "9.1.7",
    "lint-staged": "15.5.1",
    "npm-check-updates": "18.0.1",
